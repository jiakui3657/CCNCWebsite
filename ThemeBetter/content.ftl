<body class="single single-post postid-66 single-format-standard nav_fixed p_indent comment-open site-layout-2">
	<header class="header">
		<div class="container">
			<h1 class="logo">
				<a href="" title="TeaCMS">
					<img src="">TeaCMS</a>
			</h1>
			<div class="brand">
				欢迎光临<br>我们一直在努力</div>
			<ul class="site-nav site-navbar">
				<li id="menu-item-6" class="menu-item menu-item-type-custom menu-item-object-custom current-menu-item current_page_item menu-item-home menu-item-6">
					<a href="/"> <i class="fa fa-home"></i>
						首页
					</a>
				</li>
				<li id="menu-item-118" class="menu-item menu-item-type-custom menu-item-object-custom menu-item-has-children menu-item-118">
					<a>
						<i class="fa fa-file-text-o"></i>
						分类
					</a>					
					<ul class="sub-menu">
		                <#list allCategory as item>
		                    <liclass="menu-item menu-item-type-post_type menu-item-object-page menu-item-123">
		                    	<a href="${webConfig.domainName}/category/${item.alias}">${item.name}</a>
						    </li>
		                </#list>
	                </ul>
					
				</li>
				<li class="navto-search">
					<a href="javascript:;" class="search-show active">
						<i class="fa fa-search"></i>
					</a>
				</li>
			</ul>
			<i class="fa fa-bars m-icon-nav"></i>
		</div>
	</header>
<div class="site-search">
	<div class="container">
		<form method="get" class="site-search-form" action="http://demo.themebetter.com/dux/" >
			<input class="search-input" name="s" type="text" placeholder="输入关键字" value="">
			<button class="search-btn" type="submit">
				<i class="fa fa-search"></i>
			</button>
		</form>
	</div>
</div>
<section class="container">
	<div class="content-wrap">
		<div class="content">
			<header class="article-header">
				<h1 class="article-title">
					<a href="#">${articleAllVo.articleTitle}</a>
				</h1>
				<div class="article-meta">
					<span class="item">${articleAllVo.updateDate?date}</span>
					<span class="item">来源：${articleAllVo.userNicename}</span>
					<span class="item">
						分类：
						<#list articleAllVo.categories as category >
						<a href="" rel="">${category.name}</a>
						</#list>
					</span>
					<span class="item post-views">阅读(${articleAllVo.pageView})</span>
					<span class="item">评论(${articleAllVo.praise})</span>
					<span class="item"></span>
				</div>
			</header>
			<article class="article-content">
						${articleAllVo.articleContent}
			</article>
			<div class="text-center">
                <button class="btn btn-danger">打赏： 【TeaCMS】</button>
                <button class="btn btn-info">点赞：【TeaCMS:发】</button>
           </div>
			<div class="article-tags">
				标签：
				<#list articleAllVo.tags as tags>
					<a href="" rel="tag">${tags.name}</a>
				</#list>
			</div>
		<div class="teacms-box teacms-comment">
            <h3>评论</h3>
            <!-- 多说评论框 start -->
            <div class="ds-thread" data-thread-key="${articleAllVo.id}" data-title="${articleAllVo.articleTitle}"
                 data-url="${webConfig.domainName}/${articleAllVo.id}.html"></div>
            <!-- 多说评论框 end -->
            <!-- 多说公共JS代码 start (一个网页只需插入一次) -->
            <script type="text/javascript">
                var duoshuoQuery = {short_name: "${webConfig.shortName}"};
                (function () {
                    var ds = document.createElement('script');
                    ds.type = 'text/javascript';
                    ds.async = true;
                    ds.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//static.duoshuo.com/embed.unstable.js';
                    ds.charset = 'UTF-8';
                    (document.getElementsByTagName('head')[0]
                    || document.getElementsByTagName('body')[0]).appendChild(ds);
                })();
            </script>
            <!-- 多说公共JS代码 end -->
        </div>
	</div>
</div>
<aside class="sidebar">
		<div class="widget widget_ui_textasb">
				<a class="style02" href="/theme/dux"> <strong>网站公告</strong>
					<h2>TeaCMS</h2>
					<p>
						
					</p>
				</a>
			</div>
			<div class="widget widget-tops">
				<h3>网站广告</h3>
				<ul class="widget-navcontent">
					<li class="item item-01 active">
			            <div class="panel-body">
			                <a href="">
			                    <img src="http://www.xiaobingby.com/wp-content/uploads/2016/04/qcloud-logo.jpg" alt="" class="img-responsive"/>
			                </a>
			            </div>
					</li>
				</ul>
			</div>
			
			<div class="widget widget_ui_posts">
				<h3>最新文章</h3>
                <ul>
				<#assign n=0>
				<#list newArticleTop6 as item>
					<#assign n=n+1>
                    <li>
                        <a href="${webConfig.domainName}/${item.id}.html">
							<span class="thumbnail">
								<img src="${item.articleImg}" class="thumb"></span>
                            <span class="text">
							${item.articleTitle}
							</span>
                            <span class="muted">${item.updateDate?date}</span>
                            <span class="muted">评论(${item.praise})</span>
                        </a>
                    </li>
					<#if n = 3 >
						<#break>
					</#if>
				</#list>
                </ul>
				
			</div>
			<div class="widget widget_ui_posts">
				<h3>热门文章</h3>
                <ul>
				<#assign n=0>
				<#list viewArticleTop6 as item>
					<#assign n=n+1>
                    <li>
                        <a href="${webConfig.domainName}/${item.id}.html">
							<span class="thumbnail">
								<img src="${item.articleImg}" class="thumb"></span>
                            <span class="text">
							${item.articleTitle}
							</span>
                            <span class="muted">${item.updateDate?date}</span>
                            <span class="muted">评论(${item.praise})</span>
                        </a>
                    </li>
					<#if n = 3 >
						<#break>
					</#if>
				</#list>
                </ul>
			</div>
			<div class="widget widget_ui_ads">
				<!-- 广告 -->
			</div>

			<div class="widget widget_ui_tags">
				<h3>热门标签</h3>
				<div class="items">
					<#list allTag as item>
	                    <a href="${webConfig.domainName}/tag/${item.alias}">${item.name}</a>
	                </#list>
				</div>
			</div>
			<div class="widget widget_ui_statistics">
				<!-- <h3>网站统计</h3> -->
			</div>
		</aside>
</section>

